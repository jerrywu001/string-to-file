{
  "name": "string-to-file",
  "version": "0.1.2",
  "description": "Transform string to file in client side",
  "keywords": [
    "form data",
    "form",
    "base64",
    "file",
    "string to file"
  ],
  "homepage": "https://string-to-file.js-bridge.com",
  "bugs": {
    "url": "https://github.com/jerrywu001/string-to-file/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jerrywu001/string-to-file.git"
  },
  "license": "MIT",
  "author": "jerrywu001 <57242263@163.com>",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": [
      "./*",
      "./*.d.ts"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "workspaces": [
    "playground"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf -rf ./node_modules && rimraf -rf playground/node_modules && rimraf -rf package-lock.json",
    "dev": "npm run dev -w playground",
    "preview:dev": "npm run preview -w playground",
    "build:dev": "npm run build -w playground",
    "dev:pack": "npm run dev:usepack -w playground",
    "lint": "eslint --ext .ts,.tsx ./src",
    "prepare": "chmod a+x .husky/* && husky install",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --tag --push",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc && vue-tsc"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/plugin-transform-typescript": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@swc/core": "^1.3.75",
    "@testing-library/jest-dom": "^5.17.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/ui": "^0.34.1",
    "@vue/babel-plugin-jsx": "^1.1.5",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.3",
    "bumpp": "^9.1.1",
    "core-js": "^3.32.0",
    "esbuild-plugin-babel": "^0.2.3",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-vue": "^9.16.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "npm": "^9.8.1",
    "rimraf": "^5.0.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^0.34.1",
    "vue-tsc": "^1.8.8"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16"
  }
}
